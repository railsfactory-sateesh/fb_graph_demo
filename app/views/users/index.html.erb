<html>
	<head></head>
	<table border="2">
		<thead>
			<th>Access Token</th>
			<th>Username</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Gender</th>
		</thead>
		<% if @users.present? %>
		<% @users.each do |user| %>
		<tr>
			<td><%= user.access_token%></td>r <td><%= user.username%></td>
			<td><%= link_to user.first_name, "/users/#{user.first_name}", :method => :get %></td>
			<td><%= user.last_name%></td>
			<td><%= user.gender%></td>
		</tr>
		<% end %>
		<% else %>
		<tr>
			<td colspan="5" style="text-align: center;"> No Users Presnet </td>
		</tr>
		<% end %>
	</table>
	<%= link_to "New User", new_user_path%>
</html>

